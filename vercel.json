{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/"
    },
    {
      "src": "/index.(.*)",
      "dest": "/public/index.$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
